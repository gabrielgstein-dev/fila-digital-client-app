{
  "name": "fila-client-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": ">=8.0.0",
    "node": ">=20.0.0"
  },
  "scripts": {
    "preinstall": "node scripts/check-package-manager.js",
    "start": "expo start",

    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "test:e2e": "playwright test",
    "test:e2e:google": "node scripts/run-e2e-tests.js",
    "test:e2e:google-quick": "playwright test e2e/google-auth-service.e2e.spec.ts -g \"deve usar usePKCE corretamente\"",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug"
  },

  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/native": "^7.1.6",
    "axios": "^1.11.0",
    "expo": "~53.0.22",
    "expo-auth-session": "^6.2.1",
    "expo-build-properties": "^0.14.8",
    "expo-constants": "^17.1.7",
    "expo-crypto": "^14.1.5",
    "expo-font": "~13.3.2",
    "expo-linking": "~7.1.7",
    "expo-notifications": "^0.31.4",
    "expo-router": "~5.1.5",
    "expo-splash-screen": "~0.30.10",

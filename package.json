    "react-native-web": "~0.20.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@playwright/test": "^1.55.0",
    "@types/react": "~19.0.10",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.10",
    "only-allow": "^1.2.1",
    "puppeteer": "^24.17.0",
